{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Guild",
  "description": "A WoWFinder Guild",
  "type": "object",
  "properties": {
    "region": {
      "type": "string"
    },
    "realm": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "members": {
      "type": "array",
      "items": {
        "type":"object",
        "properties": {
          "character": {
            "type":"object",
            "properties": {
              "name": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  }
}